# Lista para armazenar os itens
itens = []

#Solicite e armazene os itens ao usuÃ¡rio
#print("Por favor, insira os nomes dos 3 itens que o personagem possui:")
for i in range(3):
    item = input()
    itens.append(item)
    
# Exibe a lista de itens
print("Lista de itens:")
for item in itens:
    print(f"- {item}")
